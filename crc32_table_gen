#include<iostream>
#include<string>
#include <iomanip>
static uint32_t crc_table[256];
int main()
{
  u_int32_t c;
	int n, k;
//‭1 0100 1100 0001 0001 1101 1011 0111‬
	for (n = 0; n < 256; n++) {
		c = (u_int32_t) n;
    for (k = 0; k < 8; k++) 
    {
			if (c & 1)
				//c = 0x04c11db7L ^ (c >> 1);//0xedb88320L
        {          
				  c = (0x1EDC6F41L ^(c>>1));//0xedb88320L   0x04c11db7L
        }
			else
				c = c >> 1;       
		}
 crc_table[n] = ~c;
	}
   for (size_t i = 0; i < 256; i++) {
        std::cout << std::setfill('0') << std::setw(8) << std::hex << crc_table[i];
        if (i % 4 == 3)
            std::cout << std::endl;
        else
            std::cout << ", ";
    }

  return 0;
}
